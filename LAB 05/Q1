#include<stdio.h>
int main () {
float CF;
float FG;
float Calories;
float Percent;
printf("Enter grams of fat: ");
scanf("%f", &FG);
printf("Enter calories: ");
scanf("%f", &Calories);
if (FG > 0){ 
  if (Calories > 0){
      CF = (FG*9);
      if (CF < Calories){
      Percent = ((CF/Calories)*100);
      printf("Percentage of calories that come from fat: %.2f", Percent);}
      else {printf("Number of calories from fat cannot be greater than the total number of calories");}
	  }
else 
  {printf("Invalid calories entry");}}
else
  {printf("Invalid fat grams entry");}

return 0;

}
