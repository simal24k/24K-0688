#include<stdio.h>
int main() {
float score;
printf("Enter your score: ");
scanf("%f",&score); 
(score > 4.0 || score < 0.0) ? printf("Invalid Entry, please enter a value between 0-4"):
(score >= 3.5) ? printf("Highest honors for subject"):
(score >= 3.0) ? printf("Dean's list for semester"):
(score >= 2.0) ? printf("(no message)"):
(score >= 1.0) ? printf("On probation for next semester"):
printf("Failed semester-registration suspended");
return 0;
}
