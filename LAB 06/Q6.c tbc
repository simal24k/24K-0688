#include<stdio.h>
int main(){
int i;

for(i=1; i<7;i++){
	if((i==1)||(i==6)){
	printf("*           *\n");}
	else{
	printf("*  *  *  *  *\n");}}
return 0;
}

******
#include<stdio.h>
int main(){
int i=1;
do{
	i++;
	printf("*  *  *  *  *\n");}
while(((i!=1)&&(i!=6)) && (i<6));
 printf("*           *\n");

return 0;
}
